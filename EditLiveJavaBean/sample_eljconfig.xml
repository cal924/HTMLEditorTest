        <plugin name="tableToolbar" />
        <plugin name="accessibility" />

        <!-- Enterprise Edition only -->
        <plugin name="imageEditor" />
        <plugin name="BrokenHyperlinkReport" />
        <plugin name="commenting" />
        <plugin name="templateBrowser" />

    </plugins>

