<!--
This file customizes and configures EditLive!

TIP: this file can be dynamically generated using ASP, JSP or PHP to achieve runtime changes to settings
-->
<editlive>

    <!-- Default content for the editor -->
    <document>
        <html>

            <!--
            Default document header
            -->
            <head>
